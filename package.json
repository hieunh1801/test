{
  "name": "spmed-portal-desktop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 61126 -o",
    "start:staging": "ng serve --configuration staging --port 61126 -o",
    "start:prod": "http-server -p 8080 -c-1 dist/spmed-portal-desktop -o",
    "start:pwa": "ng build --prod --aot --buildOptimizer && http-server -p 8080 -c-1 dist/spmed-portal-desktop -o",
    "i18n": "ngx-translate-extract --match TRANSLATE --input ./src --output ./src/assets/i18n/*.json --clean --sort --marker marker",
    "build": "ng build --prod --aot --buildOptimizer",
    "build:staging": "ng build --configuration staging",
    "build:prod": "ng build --prod --aot --buildOptimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~10.0.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@angular/service-worker": "~10.0.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "echarts": "^5.2.2",
    "ngx-echarts": "^7.0.2",
    "ngx-joyride": "^2.4.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@juggle/resize-observer": "^3.3.1",
    "@types/chart.js": "^2.9.28",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0-next.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
